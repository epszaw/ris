{
  "extends": ["standard", "prettier", "prettier/standard"],
  "plugins": ["prettier"],
  "env": {
    "jest": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "semi": false,
        "singleQuote": true,
        "trailingComma": "all"
      }
    ]
  }
}
